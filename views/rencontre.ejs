<section>
<div class="container">
    <div class="row">
      <ul class="thumbnails list-unstyled">
        <% for(var i = 1; i <= user[0]; i++) { %>
        <li class="col-md-3 col-sm-5">
          <div class="thumbnail" style="padding: 0">
            <div style="padding:4px">
              <img alt="300x200" style="width: 100%" src="<%= user[i].photo1 %>">
            </div>
            <div class="caption">
              <h2><%= user[i].nom %></h2>
              <h2><%= user[i].prenom %></h2>

              <p>Age : <%= user[i].age %></p>
              <p><i class="icon icon-map-marker"></i> Place, Country</p>
            </div>
            <div class="modal-footer" style="text-align: left">
              <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= user[i].populatite %>%;">
                    <span class="sr-only"><%= user[i].popularite %>% Complete</span>
                </div>
              </div>
              <div class="row">
                <div class="col-md-4"><b>60%</b><br/><small>FUNDED</small></div>
                <div class="col-md-4"><b>$400</b><br/><small>PLEDGED</small></div>
                <div class="col-md-4"><b>18</b><br/><small>DAYS</small></div>
              </div>
            </div>
          </div>

        </li>
        <% } %>

      </ul>
    </div>
</div>
